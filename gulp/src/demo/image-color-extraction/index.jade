  script(src="https://cdnjs.cloudflare.com/ajax/libs/dat-gui/0.5.1/dat.gui.min.js")
  script(src="/demo/assets/js/image-color-extraction/rgbaster.js", type="text/javascript")
  script(src="/demo/assets/js/image-color-extraction/script.js", type="text/javascript")

