
//- テンプレート継承
extends /demo/assets/_layouts/demo

//- meta設定
//- block vars
//-   - var pageID = "top"

//- _data.json参照してmeta-head呼び出し
block meta-head
  +meta-head(meta.imagecolorextraction)

//- ページ個別のCSS
block append style
  link(rel="stylesheet", href="#{path.css}image-color-extraction/style.css")

//- ページ個別のJS
block append script
